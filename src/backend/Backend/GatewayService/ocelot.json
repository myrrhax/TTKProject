{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5000
        }
      ],
      "UpstreamPathTemplate": "/api/auth/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST" ],
      "SwaggerKey": "auth"
    },
    {
      "DownstreamPathTemplate": "/api/users/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5000
        }
      ],
      "UpstreamPathTemplate": "/api/users/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "PATCH", "DELETE" ],
      "SwaggerKey": "auth"
    },
    {
      "DownstreamPathTemplate": "/tasks/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5010
        }
      ],
      "UpstreamPathTemplate": "/api/tasks/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "PATCH", "DELETE" ],
      "SwaggerKey": "tasks"
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "UpstreamPathTemplate": "/api/info/{everything}",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5030
        }
      ],
      "SwaggerKey": "information",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "PATCH", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/images/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5020
        }
      ],
      "UpstreamPathTemplate": "/api/images/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST" ],
      "SwaggerKey": "images"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "auth",
      "Config": [
        {
          "Name": "Auth Service",
          "Version": "v1",
          "Url": "http://localhost:5000/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "tasks",
      "Config": [
        {
          "Name": "Tasks Service",
          "Version": "v1",
          "Url": "http://localhost:5010/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "information", 
      "Config": [
        {
          "Name": "information", 
          "Version": "v1",
          "Url": "http://localhost:5030/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "images",
      "Config": [
        {
          "Name": "Image Service",
          "Version": "v1",
          "Url": "http://localhost:5020/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5004"
  }
}
